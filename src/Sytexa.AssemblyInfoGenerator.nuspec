<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>Sytexa.AssemblyInfoGenerator</id>
        <version>1.0.0</version>
        <title>AssemblyInfo Generator</title>
        <authors>Sytexa LLC</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Auto-generates a project's AssemblyInfo.cs file and auto-increments the build number of the 
        assembly version with every build attempt.</description>
        <releaseNotes>Initial v1.0.0 release of the package.</releaseNotes>
        <copyright>Copyright ©2020 Sytexa LLC</copyright>
        <dependencies>
            <group targetFramework=".NETCoreApp3.1" />
        </dependencies>
    </metadata>
    <files>
        <file src="readme.txt"/>
        <file src="build\**" target="build"/>
        <file src="content\**" target="content"/>
        <file src="tools\**" target="tools"/>
    </files>
</package>